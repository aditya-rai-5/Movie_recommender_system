          "from sklearn.model_selection import train_test_split\n",
          "import numpy as np\n",
          "from sklearn.neighbors import KNeighborsClassifier\n",
          "from sklearn.preprocessing import StandardScaler\n",
          "from sklearn.pipeline import Pipeline\n",
          "from sklearn.decomposition import TruncatedSVD\n",
          "from sklearn.metrics import accuracy_score, precision_score, recall_score, f1_score\n",
          "\n",
          "def categorize_rating(rating):\n",
          "    if rating > 8.5:\n",
          "        return 'Very High'\n",
          "    elif rating > 7.0:\n",
          "        return 'High'\n",
          "    elif rating > 5.9:\n",
          "        return 'Medium'\n",
          "    elif rating > 4.5:\n",
          "        return 'Low'\n",
          "    else:\n",
          "        return 'sabse kharab hai bhai'\n",
          "    \n",
          "features = data[['popularity', 'vote_average', 'vote_count', 'score']]\n",
          "target = data['vote_average'].apply(categorize_rating)\n",
          "\n",
          "train_data, test_data, train_target, test_target = train_test_split(\n",
          "    features, target, test_size=0.2, random_state=42\n",
          ")#split the data in train and test dataset\n",
          "\n",
          "knn_pipeline = Pipeline([\n",
          "    ('scaler', StandardScaler()),\n",
          "    ('svd', TruncatedSVD(n_components=2)),\n",
          "    ('knn', KNeighborsClassifier(n_neighbors=10, metric='cosine'))\n",
          "])\n",
          "\n",
          "knn_pipeline.fit(train_data, train_target)\n",
          "\n",
          "data.loc[:, 'predicted_class'] = knn_pipeline.predict(features)\n",
          "\n",
          "y_pred = knn_pipeline.predict(test_data)\n",
          "\n",
          "accuracy = accuracy_score(test_target, y_pred)\n",
          "precision = precision_score(test_target, y_pred, average='weighted')\n",
          "recall = recall_score(test_target, y_pred, average='weighted')\n",
          "f1 = f1_score(test_target, y_pred, average='weighted')\n",
          "\n",
          "print(f\\\"Accuracy: {accuracy:.4f}\\\")\n",
          "print(f\\\"Precision: {precision:.4f}\\\")\n",
          "print(f\\\"Recall: {recall:.4f}\\\")\n",
          "print(f\\\"F1 Score: {f1:.4f}\\\")\n"
