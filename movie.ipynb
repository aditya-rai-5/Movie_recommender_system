        "import nltk\n",
        "from nltk.corpus import stopwords\n",
        "from nltk.tokenize import word_tokenize\n",
        "\n",
        "nltk.download('stopwords')\n",
        "nltk.download('punkt_tab')\n",
        "\n",
        "stopwords=set(stopwords.words('english'))\n",
        "\n",
        "def remove_stopwords(text):\n",
        "    if isinstance(text, str):  # ensure it's a string\n",
        "        words = word_tokenize(text.lower()) #used to tokenize the word by which we can remove the stopwords using the below line\n",
        "        filtered_words = [word for word in words if word not in stopwords]  #remove stopwords from the sentence\n",
        "        return \" \".join(filtered_words)\n",
        "\n",
        "data['tags']=data['tags'].apply(remove_stopwords)"
      ],
