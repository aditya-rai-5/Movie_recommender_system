      "source": [
        "def score(row, C, m, alpha=0.1):\n",
        "    vote_average = row['vote_average']\n",
        "    vote_count = row['vote_count']\n",
        "    popularity = row['popularity']\n",
        "\n",
        "    weight = vote_count / m if vote_count < m else 1\n",
        "\n",
        "    return (weight * vote_average) + ((1 - weight) * C) + (alpha * popularity) #it gives the score of a movie. actually i take the formula from chatgpt\n",
        "\n",
        "\n",
        "C = data['vote_average'].mean()\n",
        "m = data['vote_count'].quantile(0.85)\n",
        "\n",
        "data['score'] = data.apply(lambda row: score(row, C, m), axis=1)"
      ],
