    {
      "cell_type": "code",
      "source": [
        "def hybrid_recommendations(movie, k_content=3, k_collaborative=3):\n",
        "    content_recs = content_based_recommendations(movie).head(k_content)\n",
        "    collaborative_recs = collaborative_recommendations(movie, k=k_collaborative)\n",
        "    combined_recs = pd.concat([content_recs, collaborative_recs]).drop_duplicates(subset=['movie_id'])\n",
        "    return combined_recs[['title', 'movie_id', 'vote_average', 'vote_count']].head(k_content + k_collaborative) #this returns the mixture of content-based and collaborative bbased movies (hybrid recommendations)"
      ],
